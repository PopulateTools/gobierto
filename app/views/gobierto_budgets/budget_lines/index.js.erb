$('[data-budget-lines]').html('<%=j render partial: 'gobierto_budgets/budget_lines/index', locals: { place_budget_lines: @place_budget_lines, kind: @kind, area_name: @area_name, year: @year } %>');
$('[data-budget-lines-tab]').attr({'tabindex': -1, 'aria-selected': 'false'}).parent().removeClass('active');
$('[data-budget-lines-tab="<%= @kind %>"]').attr({'tabindex': 0, 'aria-selected': 'true'}).parent().addClass('active');
document.getElementById('detail_tab').removeAttribute('hidden');
