<div class="<%= @modal ? 'modal zoom-anim-dialog modal_background' : 'column' %>"
     style="<%= "background-image: url(#{ image_url('images/illustrations/emojis.svg')})" if @modal %>">
  <div class="pure-g">
    <div class="pure-u-1 pure-u-md-11-24 padded_line_box m_v_2 ">
      <% @user_session_forms.each do |strategy_name, user_session_form| %>
        <%= render partial: "user/custom_sessions/#{strategy_name}/new_form", locals: { user_session_form: user_session_form } %>
      <% end %>
    </div>
  </div>
</div>
